{
  "name": "bmd/chex",
  "description": "A Configurable Framework for Health Checks",
  "keywords": ["healthz","health checks", "laravel"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Chex\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
